        android:paddingRight="@dimen/dp16"
        android:paddingTop="@dimen/dp12">

        <TextView
            android:id="@+id/tv1"
            style="@style/size11color666666"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
